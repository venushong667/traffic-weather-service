// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Camera {
  id String @id
  latitude Float @unique
  longitude Float @unique
  route String
  neighborhood String
  region String
}